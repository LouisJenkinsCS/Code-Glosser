{
    "LANG_NAME": "OpenBSD Packet Filter",
    "COMMENT_SINGLE": [
        "#"
    ],
    "COMMENT_MULTI": [],
    "COMMENT_REGEXP": {
        "1": "\/\\$\\{[^\\n\\}]*?\\}\/i",
        "2": "\/<<-?\\s*?('?)([a-zA-Z0-9]+)\\1\\n.*\\n\\2(?![a-zA-Z0-9])\/siU",
        "3": "\/\\\\['\"]\/siU"
    },
    "CASE_KEYWORDS": 0,
    "QUOTEMARKS": [
        "\""
    ],
    "HARDQUOTE": [
        "'",
        "'"
    ],
    "HARDESCAPE": [
        "\\'"
    ],
    "ESCAPE_CHAR": "",
    "ESCAPE_REGEXP": {
        "1": "#\\\\[nfrtv\\$\\\"\n]#i",
        "2": "#\\$[a-z_][a-z0-9_]*#i",
        "3": "\/\\$\\{[^\\n\\}]*?\\}\/i",
        "4": "\/\\$\\([^\\n\\)]*?\\)\/i",
        "5": "\/`[^`]*`\/"
    },
    "KEYWORDS": {
        "1": [
            "pass"
        ],
        "2": [
            "block"
        ],
        "3": [
            "quick",
            "keep",
            "state",
            "antispoof",
            "table",
            "persist",
            "file",
            "scrub",
            "set",
            "skip",
            "flags",
            "on"
        ],
        "4": [
            "in",
            "out",
            "proto"
        ]
    },
    "SYMBOLS": [
        "(",
        ")",
        "[",
        "]",
        "!",
        "@",
        "%",
        "&",
        "*",
        "|",
        "\/",
        "<",
        ">",
        ";;",
        "`",
        "="
    ],
    "CASE_SENSITIVE": [
        false,
        true,
        true,
        true,
        true
    ],
    "STYLES": {
        "KEYWORDS": {
            "1": "color: #009900; font-weight: bold;",
            "2": "color: #990000; font-weight: bold;",
            "3": "color: #7a0874;",
            "4": "color: #336699;"
        },
        "COMMENTS": [
            "color: #666666; font-style: italic;",
            "color: #800000;",
            "color: #cc0000; font-style: italic;",
            "color: #000000; font-weight: bold;"
        ],
        "ESCAPE_CHAR": {
            "1": "color: #000099; font-weight: bold;",
            "2": "color: #007800;",
            "3": "color: #007800;",
            "4": "color: #007800;",
            "5": "color: #780078;",
            "HARD": "color: #000099; font-weight: bold;"
        },
        "BRACKETS": [
            "color: #7a0874; font-weight: bold;"
        ],
        "STRINGS": {
            "0": "color: #CC0000;",
            "HARD": "color: #CC0000;"
        },
        "NUMBERS": [
            "color: #ff00cc;"
        ],
        "METHODS": [],
        "SYMBOLS": [
            "color: #000000; font-weight: bold;"
        ],
        "REGEXPS": {
            "0": "color: #007800;",
            "1": "color: #007800;",
            "2": "color: #007800;",
            "4": "color: #007800;",
            "5": "color: #660033;",
            "6": "color: #000099; font-weight: bold;",
            "7": "color: #0000ff;"
        },
        "SCRIPT": []
    },
    "URLS": {
        "1": "",
        "2": "",
        "3": "",
        "4": ""
    },
    "OOLANG": false,
    "OBJECT_SPLITTERS": [],
    "REGEXPS": {
        "0": "\\$\\{[a-zA-Z_][a-zA-Z0-9_]*?\\}",
        "1": "\\$[a-zA-Z_][a-zA-Z0-9_]*",
        "2": "(?<![\\.a-zA-Z_\\-])([a-zA-Z_][a-zA-Z0-9_]*?)(?==)",
        "4": "\\$[*#$\\-\\?!]",
        "5": "(?<=\\s)--?[0-9a-zA-Z\\-]+(?=[\\s=]|$)",
        "6": "([0-9]{1,3}\\.){3}[0-9]{1,3}",
        "7": "(&lt;(.*)&gt;)"
    },
    "STRICT_MODE_APPLIES": 0,
    "SCRIPT_DELIMITERS": [],
    "HIGHLIGHT_STRICT_BLOCK": [],
    "TAB_WIDTH": 4,
    "PARSER_CONTROL": {
        "COMMENTS": {
            "DISALLOWED_BEFORE": "$"
        },
        "KEYWORDS": {
            "DISALLOWED_BEFORE": "(?<![\\.\\-a-zA-Z0-9_$\\#])",
            "DISALLOWED_AFTER": "(?![\\.\\-a-zA-Z0-9_%\\\/])"
        }
    }
}