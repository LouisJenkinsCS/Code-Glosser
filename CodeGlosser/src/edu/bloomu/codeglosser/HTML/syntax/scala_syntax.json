    "CASE_SENSITIVE": [
        false,
        true,
        true
    ],
    "STYLES": {
        "KEYWORDS": {
            "1": "color: #0000ff; font-weight: bold;",
            "2": "color: #9999cc; font-weight: bold;"
        },
        "COMMENTS": {
            "1": "color: #008000; font-style: italic;",
